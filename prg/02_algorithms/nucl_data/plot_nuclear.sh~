echo 10   | nuclear > nucl_data/nd_10.dat

echo 30    | nuclear > nucl_data/nd_30.dat
echo 30a   | nuclear > nucl_data/nd_30a.dat
echo 30b   | nuclear > nucl_data/nd_30b.dat
echo 30c   | nuclear > nucl_data/nd_30c.dat
echo 30d   | nuclear > nucl_data/nd_30d.dat
echo 30e   | nuclear > nucl_data/nd_30e.dat

echo 80   | nuclear > nucl_data/nd_80.dat
echo 100   | nuclear > nucl_data/nd_100.dat
echo 500   | nuclear > nucl_data/nd_500.dat
echo 1000   | nuclear > nucl_data/nd_1000.dat

echo 5000    | nuclear > nucl_data/nd_5000.dat
echo 5000a   | nuclear > nucl_data/nd_5000a.dat
echo 5000b   | nuclear > nucl_data/nd_5000b.dat
echo 5000c   | nuclear > nucl_data/nd_5000c.dat
echo 5000d   | nuclear > nucl_data/nd_5000d.dat
echo 5000e   | nuclear > nucl_data/nd_5000e.dat

cd nucl_data
gnuplot "plot_nucl_data.gp"
cd ..



